1/movies/allmovies');\n            setMovies(res.data);\n        }\n        getMovies();\n    }, [])\n\n    useEffect(() => {\n        const getGenres = async () => {\n            const res = await axios.get('http://localhost:5000/api/v1/category/genres');\n            setGenres(res.data);\n        }\n        getGenres();\n    }, [])\n\n    const movieDetail = (id) => () => {\n        if(id) {\n            history.push({\n                pathname: `/movie/${id}`,\n            })\n        }\n    }\n\n    return (\n        <React.Fragment>\n            {/* <Header myloginData={myloginData}/> */}\n            <Header/>\n            <Container>\n                <h4 className=\"mt-3 mb-3\">Popular Movies</h4>\n                <Row>\n                    <Col sm={3} md={3}>\n                        <div className=\"filter-container\">\n                            <div className=\"mb-3\" style={useStyleFilterBox}>\n                                <h5>Search Movie</h5>\n                                <div className=\"panel\">\n                                    <InputGroup className=\"mb-3\">\n                                        <FormControl\n                                        placeholder=\"Movie\"\n                                        aria-label=\"Movie\"\n                                        aria-describedby=\"basic-addon2\"\n                                        />\n                                        <InputGroup.Text id=\"basic-addon2\"><FontAwesomeIcon icon={faSearch} /></InputGroup.Text>\n                                    </InputGroup>\n                                </div>\n                            </div>\n                            <div className=\"mb-3\" style={useStyleFilterBox}>\n                                <h5>Genres</h5>\n                                <div className=\"panel\">\n                                    <ul style={{listStyleType: 'none', padding: 0}}>\n                                        {genres.map((genre) => (\n                                            <li style={useStyleListBox} key={(genre._id).toString()} data-id={genre.id}>{genre.name}</li>\n                                        ))}\n                                    </ul>\n                                </div>\n                            </div>\n                        </div>\n                    </Col>\n                    <Col sm={9} md={9}>\n                        <Row>\n                        {movies.map((movie) => (\n                            <Card style={useStyleCard} key={(movie._id).toString()} onClick={movieDetail(movie._id)} data-genreid={movie.genre_ids}>\n                                <Card.Img variant=\"top\" src={`https://image.tmdb.org/t/p/w200/` + movie.poster_path} />\n                                <Card.Body>\n                                    <Card.Title>{movie.original_title}</Card.Title>\n                                    <Card.Text style={{overflow: 'hidden', textOverflow: 'ellipsis', whiteSpace: 'nowrap'}}>{movie.overview}</Card.Text>\n                                    <Button variant=\"dark\" onClick={movieDetail(movie._id)} style={{borderRadius: 20}}>View</Button>\n                                </Card.Body>\n                            </Card>\n                        ))}\n                        </Row>\n                    </Col>\n                </Row>\n            </Container>\n            <Footer/>\n        </React.Fragment>\n    )\n}\n\nexport default MovieGridView;","/home/jyoti/Projects/moviedb/moviedb-f/src/components/Details/MovieDetails.js",["68","69"],"/home/jyoti/Projects/moviedb/moviedb-f/src/components/Details/TvShowDetails.js",["70"],"import React, { useEffect, useState } from 'react';\nimport Footer from '../Footer/Footer';\nimport Header from '../Header/Header';\nimport { Container, Row, Image, Col } from 'react-bootstrap';\nimport { useParams } from 'react-router-dom';\nimport axios from 'axios';\nimport dateFormat from 'dateformat';\n\nconst TvDetail = () => {\n    const {id} = useParams();\n    const [tvDetails, setTvDetails] = useState([])\n\n    useEffect(() => {\n        const fetchTvDetails = async () => {\n            // const res = await axios.get(`http://localhost:5000/api/v1/movies/get-movie-details/${id}`);\n            const res = await axios.get(`http://localhost:5000/api/v1/tv/tv/${id}`);\n            setTvDetails(res.data);\n        }\n        fetchTvDetails();\n    }, [])\n    console.log(tvDetails)\n\n    return (\n        <React.Fragment>\n            <Header/>\n            <Container>\n                <Row className=\"p-3\">\n                    <Col sm={9} style={{ backgroundImage: `url(\"https://image.tmdb.org/t/p/w500${tvDetails.backdrop_path}\")`, backgroundRepeat: 'no-repeat', backgroundSize: 'cover', height: 360 }}>\n                        <Image src={`https://image.tmdb.org/t/p/w500` + tvDetails.poster_path} style={{height: 360, padding: 15}}></Image>\n                    </Col>\n                    <Col sm={3}>\n                        <h5 className=\"text-uppercase\">{tvDetails.name}</h5>\n                        <h6>Released On: {dateFormat(tvDetails.first_air_date, \"mmmm dS, yyyy\")}</h6>\n\n                    </Col>\n                    <Col sm={9}>\n                        <p>{tvDetails.overview}</p>\n                    </Col>\n                </Row>\n            </Container>\n            <Footer/>\n        </React.Fragment>\n    )\n}\n\nexport default TvDetail;","/home/jyoti/Projects/moviedb/moviedb-f/src/components/Register/Register.js",[],"/home/jyoti/Projects/moviedb/moviedb-f/src/components/Header/Header.js",[],"/home/jyoti/Projects/moviedb/moviedb-f/src/components/Carousel/TvshowsCarousel.js",[],"/home/jyoti/Projects/moviedb/moviedb-f/src/components/Banner/Banner.js",["71"],"import React from 'react';\nimport { Form, Row, Col, Button, FormControl, InputGroup } from 'react-bootstrap';\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome'\nimport { faSearch } f